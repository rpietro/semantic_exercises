    {
      "@id": "Fever",
      "@type": "http://schema.org/MedicalSignOrSymptom",
      "name": "Fever"
    },
    {
      "@id": "Headache",
      "@type": "http://schema.org/MedicalSignOrSymptom",
      "name": "Headache"
    },
    {
      "@id": "SoreThroat",
      "@type": "http://schema.org/MedicalSignOrSymptom",
      "name": "Sore Throat"
    },
    {
